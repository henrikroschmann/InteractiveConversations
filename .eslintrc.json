{
  "extends": "eslint:recommended",
  "env": {
      "browser": true,
      "es2021": true,
      "jquery": true
  },
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "indent": ["error", "tab"],
      "no-undef": "off",
      "no-unused-vars": ["error", {"argsIgnorePattern": "^_"}]
  }
}